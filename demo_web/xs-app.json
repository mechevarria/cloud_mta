{
	"welcomeFile": "/demo_web/webapp/index.html",
	"authenticationMethod": "none",
	"routes": [{
		"source": "^/demo_web/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"csrfProtection": false
	}, {
		"source": "^/api/(.*)$",
		"destination": "demo_express_backend",
		"target": "$1",
		"csrfProtection": false
	}]
}